<ivysettings>
  <properties file="ivysettings.properties"/>
  <settings defaultCacheDir="${ivy.settings.dir}/ivy-cache" defaultBranch="trunk" defaultResolver="chain" latest="latest-compatible"/>  
  <triggers>
    	<ant-call target="unzip" prefix="dep" event="post-download-artifact" filter="type=zip"/>
  </triggers>
  <resolvers>
  	<chain name="chain">
  		<url name="projects">
  			<artifact pattern="http://softdevbuild:82/${repository.dir}/${ivy.artifact.pattern}" />
			<ivy pattern="http://softdevbuild:82/${repository.dir}/${ivy.pattern}" />
		</url>
		<ibiblio name="libraries" m2compatible="true" usepoms="false" /> 
	    <ibiblio name="java-net-maven2" root="http://download.java.net/maven/2/" m2compatible="true" />
  	</chain>
  </resolvers>
  
</ivysettings>
