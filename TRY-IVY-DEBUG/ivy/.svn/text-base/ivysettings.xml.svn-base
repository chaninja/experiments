<ivysettings>
  <properties file="ivysettings.properties"/>
  <settings defaultCacheDir="${ivy.settings.dir}/ivy-cache" defaultResolver="chain" latest="latest-compatible"/>  
  <credentials realm="Artifactory Realm" username="softdev" passwd="bug" />
  <triggers>
    	<ant-call target="unzip" prefix="dep"
          event="post-download-artifact" filter="type=zip"/>
  </triggers>
  <resolvers>
  	<chain name="chain">
		<filesystem name="projects">
  			<artifact pattern="${repository.dir}/${ivy.artifact.pattern}" />
			<ivy pattern="${repository.dir}/${ivy.pattern}" />
		</filesystem>
		<ibiblio name="libraries" m2compatible="true" usepoms="false" /> 
  	</chain>
  </resolvers>
</ivysettings>

